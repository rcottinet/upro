{
  "name": "fronted",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc --loglevel=error",
    "prepare": "husky install",
    "cy:install": "cypress install",
    "cy:run": "cypress run --headless --browser chrome",
    "cy:open": "cypress open",
    "test": "npm run cy:run",
    "test:e2e:ci": "start-server-and-test dev http://localhost:3000 cy:run"
  },
  "dependencies": {
    "js-cookie": "^3.0.1",
    "next": "12.1.6",
    "next-themes": "^0.2.0",
    "prettier": "^2.6.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-feather": "^2.0.9",
    "react-query": "^3.38.1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.1",
    "@types/js-cookie": "^3.0.2",
    "@types/node": "17.0.31",
    "@types/react": "18.0.9",
    "autoprefixer": "^10.4.7",
    "cypress": "^9.6.0",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "husky": "^7.0.4",
    "postcss": "^8.4.13",
    "pretty-quick": "^3.1.3",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.24",
    "typescript": "4.6.4"
  }
}
