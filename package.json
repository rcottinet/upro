{
    "name": "fronted",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc --loglevel=error",
        "prepare": "husky install",
        "cy:install": "cypress install",
        "cy:run": "cypress run --headless --browser chrome",
        "cy:open": "cypress open",
        "test": "npm run cy:run",
        "test:e2e:ci": "start-server-and-test dev http://localhost:3000 cy:run"
    },
    "dependencies": {
        "js-cookie": "^3.0.1",
        "next": "12.0.10",
        "next-themes": "^0.0.15",
        "prettier": "^2.4.1",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-feather": "^2.0.9",
        "react-query": "^3.32.1"
    },
    "devDependencies": {
        "@tailwindcss/forms": "^0.4.0",
        "@types/js-cookie": "^3.0.0",
        "@types/node": "17.0.17",
        "@types/react": "17.0.39",
        "autoprefixer": "^10.4.0",
        "cypress": "^9.0.0",
        "eslint": "8.8.0",
        "eslint-config-next": "12.0.10",
        "husky": "^7.0.0",
        "postcss": "^8.4.4",
        "pretty-quick": "^3.1.1",
        "start-server-and-test": "^1.14.0",
        "tailwindcss": "^3.0.0",
        "typescript": "4.5.5"
    }
}
